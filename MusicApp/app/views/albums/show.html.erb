<h1><%= @album.name %></h1>
<h2>By: <%= link_to "#{@album.band.name}", band_url(@album.band.id), method: :get %></h2>
<h2>Tracks</h2>
<% @album.tracks.each do |track| %>
  <%= link_to "#{track.name}", track_url(track.id), method: :get %><br>
<% end %>
<br>
<%= link_to "Add a track", new_album_track_url(@album.id), method: :get %>
<br><br>
<%= link_to "Update album info", edit_album_url(@album.id), method: :get %>
